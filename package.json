{
  "name": "personal-site-amp",
  "version": "0.0.1",
  "description": "My Personal Website. Built in AMP!",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all build --parallel serve watch",
    "build": "node src/build.js",
    "serve": "npx http-server ./dist",
    "watch": "npx watch 'npm run build' ./src --interval=1",
    "lint": "prettier --write src/**/*.css src/**/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern \"**/*.*(js|css)\""
    }
  },
  "keywords": [
    "AMP",
    "Aaronthedev",
    "torch2424",
    "Aaron",
    "Turner"
  ],
  "author": "Aaron Turner",
  "license": "Apache-2.0",
  "devDependencies": {
    "clean-css": "^4.2.1",
    "cpy": "^7.2.0",
    "http-server": "^0.11.2",
    "husky": "^2.4.0",
    "mkdirp": "^0.5.1",
    "mustache": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "sakura.css": "^1.0.0"
  }
}
